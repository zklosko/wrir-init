generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model shows {
  showID    Int       @id @default(autoincrement())
  title     String
  show      String?
  datestamp String
  dateunix  DateTime  @unique
  mp3       String    @unique
  ogg       String?
  type      String?   @default("other")
  showurl   String?
  poster    String?
}

model schedule {
  showID        Int  @id @default(autoincrement())
  timeslot      String  @unique
  weekday       String
  startTime     String
  showName      String
  showNameFormal String?
  type          String  @default("other")
  dj            String?
  showURL       String?
  showIcon      String?
  showIcon2     String?
}
